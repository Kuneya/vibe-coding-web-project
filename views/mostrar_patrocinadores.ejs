<%- include('template/cabecera',{tituloPagina:'Patrocinadores',estilo:'deportistas.css'}) %>
<%- include('template/navbar') %>
<section class="container my-5">
  <h2 class="text-center mb-4">Patrocinadores Activos</h2>
  <div class="row">
    <% patrocinadores.forEach(patrocinador => { %>
      <div class="col-md-6 col-lg-4 mb-4">
        <div class="card h-100 shadow-sm">
          <div class="card-body">
            <h5 class="card-title">
              <%= patrocinador.nombre %>
            </h5>
            <p><strong>email:</strong> <%= patrocinador.email %></p>
          </div>
        </div>
      </div>
    <% }) %>
  </div>
</section>
  <%- include('template/footer') %>
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"></script> 
</body>
</html>